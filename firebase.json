{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/timestamp",
        "function": "app"
      },
      {
        "source": "/home",
        "function": "app"
      },
      {
        "source": "/contact",
        "function": "app"
      },
      {
        "source": "/thingstodo",
        "function": "app"
      }]
  }
}
